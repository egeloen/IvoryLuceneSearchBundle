{
    "name": "egeloen/lucene-search-bundle",
    "type": "symfony-bundle",
    "description": "Provides a zend lucene search integration for your Symfony2 Project.",
    "keywords": [ "lucene search" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/config": "^2.3|^3.1",
        "symfony/dependency-injection": "^2.3|^3.1",
        "symfony/filesystem": "^2.3|^3.1",
        "symfony/framework-bundle": "^2.3|^3.1",
        "zendframework/zendsearch": "^2.0@rc"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "satooshi/php-coveralls": "~1.0",
        "symfony/phpunit-bridge": "^2.7|^3.1",
        "symfony/yaml": "^2.3|^3.1"
    },
    "autoload": {
        "psr-4": { "Ivory\\LuceneSearchBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
